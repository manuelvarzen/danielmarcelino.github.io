author = {Sartori, G},
file = {:Users/daniel/Documents/Mendeley Desktop/Journal of Theoretical Politics/Sartori - 1991 - Comparing and Miscomparing.pdf:pdf},
journal = {J. Theor. Polit.},
number = {3},
