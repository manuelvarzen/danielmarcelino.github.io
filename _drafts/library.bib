@misc{Haynes1997,
abstract = {Haynes, Gurian, and Nichols examine the influence of money in presidential nominating campaigns. Previous research has indicated that money is more important in nominating campaigns in which party identification and ideological differentiation are less useful factors. The consensus of previous research is that money is a necessary, though not sufficient condition for presidential nominating contests. The authors create a model for the 1980 through 1988 nominating contests in which their dependent variable is the percentage of the vote that each candidate receives in the primary or caucus relative to the other candidates. The independent variable of interest is the campaign spending of each candidate as a percentage of total campaign expenditures by all the candidates. The authors hold viability, homestate, incumbency, momentum, and whether the competition is between two or more candidates constant. Haynes, Gurian, and Nichols find that candidate spending is a significant factor in vote share, even in situations when ideological appeal of the candidates is taken into account. Their model shows that a 3.3% increase in spending relative to the other candidate was equivalent to a 1% increase in vote share.},
author = {Haynes, Audrey A. and Gurian, Paul-Henri and Nichols, Stephen M.},
booktitle = {J. Polit.},
doi = {10.2307/2998223},
issn = {0022-3816},
pages = {213},
title = {{The Role of Candidate Spending in Presidential Nomination Campaigns}},
volume = {59},
year = {1997}
}
